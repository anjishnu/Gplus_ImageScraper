[{"kind": "plus#activity", "provider": {"title": "Google+"}, "title": "A star\nradiant\nshone down on me,\nto tell\nof folklore, sweet\nyet frail;\nglistening upon black\nso grimm...", "url": "https://plus.google.com/108992496501811486285/posts/WFAP93LBnkv", "object": {"resharers": {"totalItems": 0, "selfLink": "https://www.googleapis.com/plus/v1/activities/z133wln4itrffnuxi04ccniohqmue3ey3nk0k/people/resharers"}, "attachments": [{"fullImage": {"url": "https://lh6.googleusercontent.com/-c-dhL_ygszo/UffT9C0obEI/AAAAAAAABfA/hzEFSRcJY-Q/tumblr_mjb7puhWNM1rzszguo1_500.jpg", "width": 500, "type": "image/jpeg", "height": 346}, "url": "https://plus.google.com/photos/108992496501811486285/albums/5906422479588681985/5906422482002799682?authkey=CNvtwNOwtdbwvgE", "image": {"url": "https://lh6.googleusercontent.com/-c-dhL_ygszo/UffT9C0obEI/AAAAAAAABfA/hzEFSRcJY-Q/w506-h750/tumblr_mjb7puhWNM1rzszguo1_500.jpg", "width": 506, "type": "image/jpeg", "height": 750}, "content": "", "id": "108992496501811486285.5906422482002799682", "objectType": "photo"}], "url": "https://plus.google.com/108992496501811486285/posts/WFAP93LBnkv", "content": "A star<br />radiant<br />shone down on me,<br />to tell<br />of folklore, sweet<br />yet frail;<br />glistening upon black<br />so grimm,<br />dreams of awe<br />to tell.<br /><br />Serenely rising<br />luminous,<br />each of words<br />around,<br />glittering drops<br />of summer<br />down,<br />as winter rainfall<br />yearning.<br /><br />II.<br /><br />Elicit, tempting<br />mesmeric,<br />moonlit cries<br />of sallow tales;<br />tempering<br />thunder, of hollow<br />sound, at<br />paler black<br />to fade.<br /><br />Iridescent sparkle<br />to descend,<br />clear<br />untold yet<br />still; as<br />in secrecy<br />solely bright<br />to sing,<br />each old tale<br />anew.<br /><br />(Image source: tumblr)", "plusoners": {"totalItems": 4, "selfLink": "https://www.googleapis.com/plus/v1/activities/z133wln4itrffnuxi04ccniohqmue3ey3nk0k/people/plusoners"}, "replies": {"totalItems": 0, "selfLink": "https://www.googleapis.com/plus/v1/activities/z133wln4itrffnuxi04ccniohqmue3ey3nk0k/comments"}, "objectType": "note"}, "updated": "2013-07-30T15:04:33.495Z", "actor": {"url": "https://plus.google.com/108992496501811486285", "image": {"url": "https://lh6.googleusercontent.com/-dEwcQRbr4Qc/AAAAAAAAAAI/AAAAAAAACD4/FwfhDCIvBKY/photo.jpg?sz=50"}, "displayName": "Rebekka Chapman", "id": "108992496501811486285"}, "access": {"items": [{"type": "public"}], "kind": "plus#acl", "description": "Public"}, "verb": "post", "etag": "\"Mwea3qudNDf-hWWlPWi3n-YrU1Q/fGN4KeRqHf93iF3l-HH9vQBj4AA\"", "published": "2013-07-30T15:04:33.495Z", "id": "z133wln4itrffnuxi04ccniohqmue3ey3nk0k"}, "NONE"]