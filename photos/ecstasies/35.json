[{"kind": "plus#activity", "provider": {"title": "Google+"}, "title": "\u200e\"O Mary, you are more mother than queen! When I meditate upon your life, as the Gospel presents it ...", "url": "https://plus.google.com/117826537890431198027/posts/dxybfkebsWM", "object": {"resharers": {"totalItems": 0, "selfLink": "https://www.googleapis.com/plus/v1/activities/z12rgvvrckemzxzyw04cin34mvjntbd52ok0k/people/resharers"}, "attachments": [{"fullImage": {"url": "https://lh6.googleusercontent.com/-5klADn_dRSE/T76Z177YTwI/AAAAAAAABTA/9PhMU-gmLho/w288-h288/tcj.jpg", "width": 226, "type": "image/jpeg", "height": 294}, "url": "https://plus.google.com/photos/117826537890431198027/albums/5746199324581556577/5746199326470196994", "image": {"url": "https://lh6.googleusercontent.com/-5klADn_dRSE/T76Z177YTwI/AAAAAAAABTA/9PhMU-gmLho/w506-h750/tcj.jpg", "width": 506, "type": "image/jpeg", "height": 750}, "content": "tcj.jpg", "id": "117826537890431198027.5746199326470196994", "objectType": "photo"}], "url": "https://plus.google.com/117826537890431198027/posts/dxybfkebsWM", "content": "\u200e&quot;O Mary, you are more mother than queen! When I meditate upon your life, as the Gospel presents it to me, so humble and simple, I do not fear to approach you. I see you living in poverty and obscurity, with no transports or ecstasies, no splendid miracles or brilliant deeds. You show me that I, too, can follow your steps and climb the rough road of sanctity by practicing the hidden virtues. Close to you, O Mary, I like to remain little, and I get a better view there of the vanity of human greatness.&quot;<br /><br />-- St. Th\u00e9r\u00e8se of the Child Jesus", "plusoners": {"totalItems": 0, "selfLink": "https://www.googleapis.com/plus/v1/activities/z12rgvvrckemzxzyw04cin34mvjntbd52ok0k/people/plusoners"}, "replies": {"totalItems": 0, "selfLink": "https://www.googleapis.com/plus/v1/activities/z12rgvvrckemzxzyw04cin34mvjntbd52ok0k/comments"}, "objectType": "note"}, "updated": "2012-05-24T20:28:14.021Z", "actor": {"url": "https://plus.google.com/117826537890431198027", "image": {"url": "https://lh6.googleusercontent.com/-K3izitj_SFo/AAAAAAAAAAI/AAAAAAAAEjQ/wGYnQ2XSTng/photo.jpg?sz=50"}, "displayName": "Br. Paul Werley, C.O.", "id": "117826537890431198027"}, "access": {"items": [{"type": "public"}], "kind": "plus#acl", "description": "Public"}, "verb": "post", "etag": "\"Mwea3qudNDf-hWWlPWi3n-YrU1Q/UIqhKvdxZ9eYmIweFbtsVJw8fRo\"", "published": "2012-05-24T20:28:14.021Z", "id": "z12rgvvrckemzxzyw04cin34mvjntbd52ok0k"}, "NONE"]